<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
	<!-- Adds all assets in Odoo -->
        <template id="assets_frontend" name="rlm_website_custom_css assets" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">
		         <!-- All links to CSS and JS files should be added in here. -->
		         <link rel="stylesheet" href="/rlm_website_custom_css/static/src/css/custom.css"/>
		         <link rel="stylesheet" href="/rlm_website_custom_css/static/src/less/custom.less"/>
            </xpath>
        </template>
    </data>
</openerp>
